                  <div class="parrent media-body">
                    <div class="tab-content">
                      <div class="tab-pane fade active in" id="tab1">
                        <div class="media">
                          <div class="media-body">
                            <button  class="btn btn-danger" onclick="alertbox()">Click the button to display an alert box:</button>
                            <script>
function alertbox() {
    alert("I am an alert box!");
}
</script>
                          </div>
                        </div>
                      </div>
                      <div class="tab-pane fade" id="tab2">
                        <div class="media">
                          <div class="media-body">
                            <button class="btn btn-primary" onclick="confirmbox()">Click the button to display a confirm box.</button>
                            <p id="demo"></p>
                            <script>
function confirmbox() {
    var x;
    if (confirm("Press a button!") == true) {
        x = "You pressed OK!";
    } else {
        x = "You pressed Cancel!";
    }
    document.getElementById("demo").innerHTML = x;
}
</script>
                          </div>
                        </div>
                      </div>
                      <div class="tab-pane fade" id="tab3">
                        <button class="btn btn-info" onclick="promptbox()">Click the button to demonstrate the prompt box.</button>
                        <p id="demo"></p>
                        <script>
function promptbox() {
    var person = prompt("Please enter your name", "Harry Potter");
    
    if (person != null) {
        document.getElementById("demo").innerHTML =
        "Hello " + person + "! How are you today?";
    }
}
</script>
                      </div>
